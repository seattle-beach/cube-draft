{
  "name": "cube-draft",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "prebuild": "yarn build-css",
    "build": "react-scripts build && touch ./build/Staticfile",
    "start-js": "react-scripts start",
    "pretest": "yarn build-css",
    "test": "babel-node spec/run.js",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cypress": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jasmine": "^3.1.0",
    "jasmine-enzyme": "^6.0.2",
    "jsdom": "^11.11.0",
    "mock-http-server": "^1.0.0",
    "node-sass": "^4.9.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "wait-on": "^2.1.0"
  }
}
